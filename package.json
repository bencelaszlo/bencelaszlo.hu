{
  "name": "bencelaszlo.github.io",
  "author": {
    "name": "Bence László",
    "email": "bencelaszlo@protonmail.com"
  },
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "blog": "vuepress dev blog",
    "blog:build": "vuepress build blog",
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "npm run build; npm run blog:build; git add *; git commit -m \"(dist/*): Regenerate static files.\"; git subtree push --prefix dist origin master",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit --passWithNoTests"
  },
  "dependencies": {
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.12.0",
    "core-js": "^3.6.5",
    "portal-vue": "^2.1.7",
    "vue": "^2.6.11",
    "vue-feather-icons": "^5.0.0",
    "vue-loader": "^15.9.1",
    "vue-mobile-detection": "^1.0.0",
    "vue-router": "^3.1.6",
    "vuex": "^3.2.0",
    "vuex-store": "^1.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/test-utils": "^1.0.0-beta.33",
    "@vuepress/plugin-back-to-top": "^1.4.1",
    "@vuepress/plugin-blog": "^1.9.2",
    "@vuepress/theme-blog": "^1.7.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.1.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.4.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
